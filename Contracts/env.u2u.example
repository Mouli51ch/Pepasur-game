# U2U Solaris Mainnet Configuration
# Copy this file to .env for U2U deployment

# ============================================
# DEPLOYMENT CONFIGURATION
# ============================================
# Your wallet private key for deployment
PRIVATE_KEY=0xYOUR_PRIVATE_KEY_HERE

# ============================================
# PEPASUR CONTRACT CONFIGURATION
# ============================================
# Server signer address (for game logic)
SERVER_SIGNER=0xYOUR_SERVER_SIGNER_ADDRESS

# Fee recipient address (for house cut)
FEE_RECIPIENT=0xYOUR_FEE_RECIPIENT_ADDRESS

# House cut in basis points (200 = 2%)
HOUSE_CUT_BPS=200

# ============================================
# U2U NETWORK CONFIGURATION
# ============================================
# Network details
NETWORK_NAME=U2U Solaris Mainnet
RPC_URL=https://rpc-mainnet.u2u.xyz
CHAIN_ID=39
SYMBOL=U2U
BLOCK_EXPLORER=https://u2uscan.xyz

# ============================================
# CONTRACT ADDRESSES (will be populated after deployment)
# ============================================
PEPASUR_CONTRACT_ADDRESS=0x1fbA70E7A768448EC1Bf34DC53558b47De7b872c
